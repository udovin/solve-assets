{
    "name": "cpp17-gcc-11.3.0",
    "kind": "compiler",
    "config": {
        "extensions": [
            "cpp",
            "h",
            "hpp",
            "c++",
            "cxx"
        ],
        "compile": {
            "command": "g++ --std=c++17 -O2 -DONLINE_JUDGE -o solution solution.cpp",
            "environ": [],
            "source": "solution.cpp",
            "target": "solution",
            "workdir": "/home/judge"
        },
        "execute": {
            "command": "./solution",
            "environ": [],
            "target": "solution",
            "workdir": "/home/judge"
        }
    }
}